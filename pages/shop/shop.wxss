.re {
	width: 94%;
	margin-left: 3%;
}

.re .title {
	text-align: center;
	padding-top: 30rpx;
	color: #666;
	font-size: 32rpx;
}

.re .bod {
	display: flex;
	align-items: center;
	border: 2rpx solid #e54148;
