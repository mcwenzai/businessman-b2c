}

.get text {
	font-size: 30rpx;
	color: #e54148;
}

.cart {
	display: -webkit-flex;
  background: #fff;
  height: 100rpx;
	left: 0;
	bottom: 0;
	width: 100%;
	background: #fff;
	position: fixed;
	z-index: 200;
}

.cart .data {
	flex: 1;
}

.cart .data .icon {
	position: absolute;
	left: 40rpx;
	top: -20rpx;
	width: 100rpx;
	height: 100rpx;
	background: #ffffff;
	border-radius: 50%;
}

.cart .data .icon .iconfont {
	position: absolute;
	left: 19rpx;
	top: 10rpx;
	font-size: 72rpx;
	color: #e54148;
}

.cart .data .icon .count {
	position: absolute;
	left: 70rpx;
	top: -10rpx;
	font-size: 24rpx;
	line-height: 16px;
	padding: 0 10rpx;
	color: #fff;
	background: linear-gradient(to right, #e44158, #e54148);
	border-radius: 32rpx;
	height: 32rpx;
}

.cart .data .icon .none {
	color: #cccccc;
}

.cart .data .icon .none1 {
	background: #cccccc;
}

.cart .data .total {
	color: #333333;
	font-size: 34rpx;
	line-height: 100rpx;
	padding-left: 160rpx;
}

.cart .data .total text {
	color: #E54148;
	font-size: 34rpx;
}

.cart button {
	margin-top: 10rpx;
	margin-right: 20rpx;
	width: 216rpx;
	height: 80rpx;
	font-size: 30rpx;
	border-radius: 40rpx;
	background-color: #E54148;
	color: #ffffff;
}

.cart button::after {
	border: none;
}

.like {
	display: inline-block;
	width: 15px;
	height: 15px;
	background-position: -416px -63px;
}

.cartList {
	width: 100%;
	height: auto;
	display: inline-block;
}

.cartList .flexbox {
	display: flex;
}

.cartList .flexbox .shopImg {
	flex: 1;
	margin-right: 18rpx;
}

.cartList .flexbox .shopImg image {
	width: 140rpx;
	height: 120rpx;
}

.cartList .flexbox .right {
	flex: 5;
}

.cartList .flexbox .right .inside-content {
	width: 100%;
	color: #585858;
}

.order-price {
	color: #E54148;
	font-size: 28rpx;
}

.right-content {
	float: right;
}

