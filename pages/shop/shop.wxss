	margin: 40rpx 0 20rpx 0;
}

.bod .amount {
	flex: 2;
}

.amount .money {
	font-size: 30rpx;
	color: #e54148;
	line-height: 44rpx;
}

.money .label {
	font-size: 24rpx;
}

.money .num {
	font-size: 36rpx;
}

.amount .full {
	font-size: 24rpx;
	color: #666;
	line-height: 44rpx;
}

.bod .name {
	flex: 5;
}

.name .shop {
	font-size: 30rpx;
	line-height: 44rpx;
}

.name .term {
	font-size: 24rpx;
	color: #666;
	line-height: 44rpx;
}

.bod .line {
	flex: 0.4;
	display: flex;
	align-items: center;
}

.van-divider {
	border: 1rpx solid #e54148;
}

.bod .get {
	flex: 1.2;
	font-size: 32rpx;
	text-align: center;
