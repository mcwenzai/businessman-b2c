.indextip {
  position: fixed;
  right: 38rpx;
  z-index: 9;
  top: 14rpx;
}

.indextip .tip {
  width: 400rpx;
  background: #ffffff;
  border-radius: 6rpx;
  position: relative;
  display: flex;
  justify-content: flex-end;
  border: 1rpx solid #e8e8e8;
  box-shadow: 8rpx 7rpx 4rpx 0rpx rgba(195, 195, 195, 0.75);
}

.indextip .tip::before {
  content: "";
  width: 0;
  height: 0;
  border-left: 15rpx solid transparent;
  border-right: 15rpx solid transparent;
  border-bottom: 17rpx solid #e8e8e8;
  position: absolute;
  top: -17rpx;
  right: 95rpx;
}

.indextip .text {
  font-size: 22rpx;
  color: #333;
  padding: 0rpx 32rpx 8rpx 10rpx;
}

.indextip .text .icon-xiaochengxu {
  font-size: 32rpx;
  display: inline-block;
}

.indextip .icon-guanbi1 {
  color: #999999;
  font-size: 32rpx;
  position: absolute;
}

.header {
  width: 94%;
  height: 84rpx;
  padding: 5rpx 3%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  background: #fff;
}

.header .position {
  width: 152rpx;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.header .position text {
  font-size: 30rpx;
}

.header .position image {
  width: 36rpx;
  height: 36rpx;
  margin-left: 10rpx;
}

.header .search_box {
  background-color: #F3F4F8;
  border-radius: 60rpx;
  padding: 6rpx 100rpx;
  color: #666;
}

.header .search_box .iconfont {
  font-size: 34rpx;
  margin-right: 16rpx;
}

.header .search_box .seach_text {
  font-size: 30rpx;
}

.white {
  background-color: #fff;
}

.swiperCenter {
  display: flex;
  flex-direction: column;
}

.swiperCenter swiper {
  width: 96%;
  height: 300rpx;
  margin-left: 2%;
  border-radius: 40rpx;
}

.swiperCenter swiper swiper-item image {
  display: block;
  width: 100%;
  height: 100%;
}

.menuBox {
  width: 96%;
  margin-left: 2%;
  display: flex;
  align-items: center;
  align-content: center;
  height: 220rpx;
}

.menuBox .menu {
  width: 20%;
  height: 170rpx;
  display: flex;
  justify-content: center;
}

.menuBox .menu .list {
  display: flow-root;
  justify-content: center;
}

.menuBox .menu .list .menu_image {
  width: 120rpx;
  height: 120rpx;
}

.menuBox .menu .list .menu_image image {
  height: 100rpx;
  width: 100rpx;
  padding: 10rpx;
}

.menuBox .menu .list .menu_text {
  width: 120rpx;
  text-align: center;
  line-height: 60rpx;
}

.menuBox .menu .list .menu_text text {
  font-size: 26rpx;
  color: #444444;
}

.iconviewst {
  width: 94%;
  height: 145rpx;
  padding: 26rpx 3%;
  background: #fff;
}

.iconviewst swiper-item {
  height: 110rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.sw-item {
  height: 110rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.iconviewst swiper-item view {
  width: 92rpx;
  height: 100%;
}

.iconviewst swiper-item view image {
  display: block;
  width: 100%;
  height: 92rpx;
}

.iconviewst swiper-item view text {
  font-size: 24rpx;
}

.iconviewst swiper-item view.active image {
  padding: 3rpx;
  border-radius: 50%;
  box-sizing: border-box;
}

/* .iconviewst swiper-item view.active text {
