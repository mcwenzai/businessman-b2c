
  .modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
  }
  .modal-dialog {
    width: 100%;
    height: 446rpx;
    overflow: hidden;
    
    z-index: 9999;
    background: #fff;
    border-radius:  48rpx 48rpx 0 0;
    position: relative;
  }
  .modal-title {
  padding-top: 20rpx;
  font-size: 32rpx;
  font-weight: bold;
  color: #999999;
  text-align: center;
  }
  .modal-content {
  padding: 0 55rpx;
  margin-top: 50rpx;
  }
  .modal-input {
  display: flex;
  flex-direction: column;
  border-radius: 12rpx;
  font-size: 32rpx;
  color: #333;
  }
  .modal-input view{
    margin: 0 auto;
  }
  .modal-footel {
  display: flex;
  align-content: space-between;
  height: 86rpx;
  line-height: 86rpx;
  margin-top: 60rpx;
  }
  .btn-cancel {
  width: 300rpx;
  height: 88rpx;
  font-size: 28rpx;
  font-weight: 600;
  margin: 0 55rpx;
  background-color: #EEEEEE;
  border-radius: 44rpx;
  color: #666;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  }
  .confirm {
  color: #ec5300;
  }
  